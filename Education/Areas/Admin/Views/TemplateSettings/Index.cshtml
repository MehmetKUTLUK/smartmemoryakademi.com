@model IEnumerable<Education.Data.TemplateSetting>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BannerText)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Card1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Card2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Card3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Card4)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BannerText)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Card1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Card2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Card3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Card4)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
